public static List<Integer> mergeLists(List<Integer> list1, List<Integer> list2){
    // solution 
    List<Integer> result = new List<Integer>();
    for(Integer i : list1){
        result.add(list1[i]);
    }
    for(Integer j : list2){
        result.add(list2[j]);
    }
    return result.sort();
}