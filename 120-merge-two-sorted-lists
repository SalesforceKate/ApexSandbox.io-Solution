public static List<Integer> mergeLists(List<Integer> list1, List<Integer> list2){
    // solution 
    List<Integer> result = new List<Integer>();
    for(Integer i =0; i < list1.size(); i++){
        result.add(list1[i]);
    }
    for(Integer j =0; j < list2.size(); j++){
        result.add(list2[j]);
    }
    result.sort();
    return result;
}