public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    // Define list of sObjects
    List<sObject> obj = new List<sObject>();
    // return empty list if both accounts and contacts is empty
    if (accounts!=null){
        obj.addAll(accounts);
    }
    if (contacts!=null){
        obj.addAll(contacts);
    }
    return obj;

}